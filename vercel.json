{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "framework": "nextjs",
    "regions": ["iad1"],
    "env": {
      "NEXT_PUBLIC_TURSO_DATABASE_URL": "@next_public_turso_database_url",
      "NEXT_PUBLIC_TURSO_AUTH_TOKEN": "@next_public_turso_auth_token",
      "NEXTAUTH_SECRET": "@nextauth_secret",
      "GOOGLE_CLIENT_ID": "@google_client_id",
      "GOOGLE_CLIENT_SECRET": "@google_client_secret"
    }
  }